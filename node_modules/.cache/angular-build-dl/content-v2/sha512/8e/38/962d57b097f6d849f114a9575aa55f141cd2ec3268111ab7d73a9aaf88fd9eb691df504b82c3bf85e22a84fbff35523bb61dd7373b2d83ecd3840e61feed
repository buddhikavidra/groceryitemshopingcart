 },\r\n        550: { items: 3, margin: 5 },\r\n        670: { items: 4, margin: 5 },\r\n      },\r\n      autoplay: true,\r\n      loop: true,\r\n      autoplayTimeout: 3000,\r\n      lazyLoad: true,\r\n    };\r\n    this.getAllProducts();\r\n  }\r\n\r\n  getAllProducts() {\r\n    this.loading = true;\r\n    const x = this.productService.getProducts();\r\n    x.snapshotChanges()\r\n      .pipe(map((products) => products.slice(0, 5)))\r\n      .subscribe(\r\n        (products) => {\r\n          this.loading = false;\r\n          this.bestProducts = [];\r\n          products.forEach((element) => {\r\n            const y = { ...element.payload.toJSON(), $key: element.key };\r\n            this.bestProducts.push(y as Product);\r\n          });\r\n        },\r\n        (error) => {\r\n          this.toasterService.error(\"Error while fetching Products\", error);\r\n        }\r\n      );\r\n  }\r\n}\r\n","<!--Section: Products v.2-->\r\n<section class=\"section pb-3\">\r\n  <div class=\"row\">\r\n    <div class=\"col-md-6\">\r\n      <blockquote class=\"blockquote float-left bq-primary\">\r\n        <h3 class=\"bq-title\">\r\n          {{ translate.data[\"BESTPRODUCTS\"] | translate }}\r\n        </h3>\r\n      </blockquote>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <a\r\n        class=\"btn btn-default float-right\"\r\n        [routerLink]=\"['/products/all-products']\"\r\n        >{{ translate.data[\"VIEWALL\"] | translate }}</a\r\n      >\r\n    </div>\r\n  </div>\r\n\r\n  <div *ngIf=\"loading\">\r\n    <app-card-loader [loop]=\"4\" [height]=\"500\"></app-card-loader>\r\n  </div>\r\n\r\n  <owl-carousel\r\n    *ngIf=\"!loading\"\r\n    [options]=\"options\"\r\n    [items]=\"bestProducts\"\r\n    [carouselClasses]=\"['owl-theme', 'row', 'sliding']\"\r\n  >\r\n    <!--First column-->\r\n    <div\r\n      class=\"item\"\r\n      style=\"padding: 20px;\"\r\n      *ngFor=\"let product of bestProducts; let i = index\"\r\n    >\r\n      <div class=\"mb-r\">\r\n  