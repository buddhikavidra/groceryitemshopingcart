Charge = 10;\r\n   ServiceChargeForMinorBills = 0;\r\n\r\n\r\n  constructor(productService: ProductService) {\r\n    document.getElementById(\"shippingTab\").style.display = \"none\";\r\n    document.getElementById(\"billingTab\").style.display = \"none\";\r\n    document.getElementById(\"resultTab\").style.display = \"none\";\r\n\r\n    const products = productService.getLocalCartProducts();\r\n\r\n    this.checkoutProducts = products;\r\n\r\n    products.forEach((product) => {\r\n      this.totalPrice += product.productPrice; // +=\r\n      if (this.totalPrice > 5000){\r\n        console.log(this.totalPrice);\r\n        this.temp = (this.totalPrice * this.ServiceCharge) / 100;\r\n        this.prizeWithServiceCharge = this.totalPrice + this.temp;\r\n      }else{\r\n          this.ServiceChargeForMinorBills = 50;\r\n          this.temp = (this.totalPrice * this.ServiceChargeForMinorBills) / 100;\r\n          this.prizeWithServiceCharge = this.totalPrice + this.temp;\r\n      }\r\n\r\n    });\r\n  }\r\n\r\n  ngOnInit() {}\r\n}\r\n","<div class=\"container\">\r\n  <ul class=\"list-group mb-3\">\r\n    <li\r\n      class=\"list-group-item d-flex justify-content-between lh-condensed\"\r\n      *ngFor=\"let product of checkoutProducts\"\r\n    >\r\n      <div>\r\n        <img\r\n          [src]=\"product.productImageUrl\"\r\n          [alt]=\"product.productName\"\r\n          width=\"150\"\r\n          height=\"100\"\r\n        />\r\n      </div>\r\n      <div style=\"padding: 10px;\">\r\n        <h6 class=\"my-0\">{{ product.productName }}</h6>\r\n        <p>Product Seller : {{ product.productSeller }}</p>\r\n      </div>\r\n      <span class=\"text-muted\"> LKR {{ product.productPrice }}</span>\r\n    </li>\r\n    <hr />\r\n    <li class=\"list-group-item d-flex justify-content-between\">\r\n\r\n      <span>Total</span>\r\n\r\n      <strong>LKR {{ prizeWithServiceCharge }}</strong>\r\n\r\n\r\n\r\n    </li>\r\n    <li class=\"list-group-item d-flex justify-content-betw